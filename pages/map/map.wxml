<!--map.wxml-->
<map
  id="map"
  longitude="{{ longitude }}"
  latitude="{{ latitude }}"
  scale="16"
  controls="{{ controls }}"
  circles="{{ circles }}"
  markers="{{ markers }}"
  bindcontroltap="controltap"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 100vh;"></map>
